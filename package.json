{
  "name": "yamjs-snipptes",
  "displayName": "yamjs-snipptes",
  "description": "",
  "version": "0.0.13",
  "engines": {
    "vscode": "^1.38.0"
  },
  "keywords": [
    "yamjs",
    "plugin",
    "yam"
  ],
  "publisher": "xuxueliang",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/xueliangGit/yamjs-snipptes.git"
  },
  "contributes": {
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/javascript.json"
      }
    ]
  },
  "scripts": {
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.3.1",
    "vscode": "^1.1.28",
    "tslint": "^5.12.1",
    "@types/node": "^10.12.21",
    "@types/mocha": "^2.2.42"
  }
}
